<%= simple_form_for @article do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :body %>
  <%= f.input :url %>
  <%= f.input :image_url %>
  <%= f.input :publish_time %>
  <%= f.input :sentiment %>
  <%= f.input :latitude %>
  <%= f.input :longitude %>
  <%= f.button :submit %>
<% end %>
